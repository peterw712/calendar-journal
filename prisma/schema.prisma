generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}


model JournalEntry {
  id        String   @id @default(cuid())
  date      String   @unique
  body      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([date])
}
