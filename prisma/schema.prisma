generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model JournalEntry {
  id        String   @id @default(cuid())
  date      String   @unique
  body      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([date])
}
